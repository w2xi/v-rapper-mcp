{
  "name": "v-rapper-mcp",
  "version": "0.0.4",
  "description": "v-rapper MCP server for Evan You",
  "type": "module",
  "bin": {
    "v-rapper-mcp": "./dist/cli.js"
  },
  "main": "./dist/cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/w2xi/v-rapper-mcp.git"
  },
  "homepage": "https://github.com/w2xi/v-rapper-mcp",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
