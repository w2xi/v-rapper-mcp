    "inspector": "npx @modelcontextprotocol/inspector node dist/cli.js"
  },
  "devDependencies": {
    "bumpp": "^10.2.0",
    "prettier": "^3.6.2",
    "tsdown": "latest"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.15.1"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "v-rapper-mcp",
    "mcp",
    "mcp-server"
  ],
  "author": "w2xi",
  "license": "MIT"
